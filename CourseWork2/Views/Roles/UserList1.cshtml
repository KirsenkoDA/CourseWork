@model IEnumerable<CourseWork2.ViewModels.ChangeRoleViewModel>

<h2>Список пользователей</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserEmail</td>
            <td> @foreach(var role in user.UserRoles)
                {
                    <span>@role</span><span>, </span>
                }
            </td>
            <td>
                @*<a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>*@
            </td>
        </tr>
    }
</table>